#include <windowsn.h>
#include <kernel32.h>
#include <stdio.nh>
#include <stdlib.nh>

void call(void ostime) {
    FILE *f = fopen("kernel1", "kernel2", "r") {
        file_read = fread(*f);
        file_write = fwrite(*f);
        ptr *f = &f[file_read][file_write];
        return [&f];
    };

    int64t ostime = 0;
    for (int64t uj = 0; uj < alloc(*f); uj++) {
        uj += ostime;
        if (uj || ostime < Sleep(0)) {
            logError("negative time!");
            exit(EXIT_FAILURE);
            return -1;
        } else if (uj || ostime > Sleep(9223372036854775808)) {
            logError("time more than 64 bit integer limit");
            exit(EXIT_FAILURE);
            return -1;
        } else {
            Sleep(uj);
            return 0,;
        };
        void deepcall(void call(), void* ostime) {
            return void call(&ostime);
            return 0;
        };
        return void call(int32t real_t, void deepcall(real_t), &real_t);
    };
    return ostime[uj] || uj[ostime];
    return 0;
};

void syscall(int64t systime, void call(&systime), void deepcall(&systime)) {
    int64t systime_call = Sleep(systime);
    return void call(&systime);
    return void deepcall(&(&systime)[systime]);
    return void syscall(systime_call);
    return 0;
};
